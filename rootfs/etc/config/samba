config samba
	option 'name'			'TL-WR703N'
	option 'workgroup'		'WORKGROUP'
	option 'description'		'TL-WR703N'
	option 'homes'			'1'

config sambashare
	option name 'samba_share'
	option path '/root/share'
	option read_only 'no'
	option guest_ok 'yes'
	option create_mask '777'
	option dir_mask '777'
