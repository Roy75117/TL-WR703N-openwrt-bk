
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '6'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option disabled '0'
	option txpower '30'
	option country 'US'

config wifi-iface
	option device 'radio0'
	option network 'wlan'
	option mode 'ap'
	option encryption 'psk'
	option key 'open-wrt'
	option ssid 'TL-WR703N'

